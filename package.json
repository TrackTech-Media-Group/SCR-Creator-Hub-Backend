{
	"name": "template",
	"version": "1.0.0",
	"description": "A quick and easy to use template for TypeScript like projects ✏️",
	"author": "ijsKoud <github@ijskoud.dev>",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"homepage": "https://github.com/ijsKoud/project-template",
	"scripts": {
		"build": "tsc --build",
		"build:watch": "tsc --watch",
		"start:dev": "nodemon ./dist",
		"start": "prisma migrate deploy && node ./dist",
		"lint": "eslint src",
		"postinstall": "is-ci || husky install",
		"prisma": "yarn dotenv -e ./data/.env -- prisma"
	},
	"dependencies": {
		"@prisma/client": "4.10.1",
		"@sapphire/discord-utilities": "^3.0.0",
		"@snowcrystals/icicle": "^1.0.2",
		"axios": "^1.3.4",
		"colorette": "^2.0.19",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cron": "^2.2.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-angular": "^17.4.4",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/cron": "^2.0.0",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/node": "^18.14.1",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"dotenv-cli": "^7.0.0",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.1.2",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.4",
		"prisma": "^4.10.1",
		"typescript": "4.9.5"
	},
	"engines": {
		"node": ">= v18.14.2"
	},
	"packageManager": "yarn@3.4.1"
}
